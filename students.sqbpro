SELECT s.student_id, s.subject, 0, t.work_type
FROM (
    SELECT DISTINCT student_id, subject FROM grades
) s
CROSS JOIN (
    SELECT 'tk1' AS work_type UNION SELECT 'tk2'
    UNION SELECT 'rk1' UNION SELECT 'rk2'
    UNION SELECT 'r1' UNION SELECT 'r2'
    UNION SELECT 'dopusk'
    UNION SELECT 'exam'
    UNION SELECT 'FINAL'
) t
WHERE NOT EXISTS (
    SELECT 1 FROM grades g
    WHERE g.student_id = s.student_id
    AND g.subject = s.subject
    AND g.work_type = t.work_type
);
</sql><current_tab id="0"/></tab_sql></sqlb_project>
